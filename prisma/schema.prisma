generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model images {
  id          Int       @id @default(autoincrement())
  userid      BigInt?
  username    String?
  create_time DateTime?
  platform    String?
  title       String?
  page        Int?
  size        Int?
  filename    String?
  author      String?
  authorid    BigInt?
  pid         String?
  extension   String?
  rawurl      String?
  thumburl    String?
  width       Int?
  height      Int?
  guest       Boolean?  @default(false)
  r18         Boolean?  @default(false)
  ai          Boolean?  @default(false)
}

model imagetags {
  id  Int     @id @default(autoincrement())
  pid String?
  tag String?
}

model stash {
  id           Int       @id @default(autoincrement())
  original_msg String?
  create_time  DateTime?
}
